{
    "name": "antelope",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf public",
        "dev": "run-p dev:client dev:server",
        "dev:client": "cross-env NODE_ENV=development webpack-dev-server --hide-modules",
        "dev:server": "cross-env NODE_ENV=development nodemon --watch server .",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --hide-modules",
        "start": "cross-env NODE_ENV=production node .",
        "lint": "npm-run-all lint:js lint:style",
        "lint:js": "eslint . --fix",
        "lint:style": "stylelint **/*.{css,scss,sass} --fix",
        "test": "cross-env NODE_ENV=test jest",
        "test:watch": "npm test -- --watch",
        "test:update": "npm test -- --updateSnapshot",
        "test:coverage": "rimraf storage/coverage && npm test -- --coverage"
    },
    "engines": {
        "npm": ">=6.4.1",
        "node": ">=10.13.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not IE 10"
    ],
    "sideEffects": [
        "*.css",
        "*.scss",
        "*.sass"
    ],
    "keywords": [
        "react",
        "express",
        "starter"
    ],
    "author": "Vladimir",
    "license": "MIT",
